{
  "expo": {
    "splash": {
      "image": "./assets/images/logo-with-text.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F8F8"
    },
    "name": "PyStart",
    "scheme": "com.anonymous.pystart",
    "slug": "pystart",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.anonymous.pystart",
      "scheme": "com.anonymous.pystart"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9dba01c9-c254-42d5-af3c-65c8a6c6bdd7"
      }
    }
  }
}
